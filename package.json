{
    "name": "@kurlytail/gen-spring",
    "description": "SPRING project generator",
    "version": "1.3.37",
    "main": "dist/spring.min.js",
    "devDependencies": {
        "@babel/cli": "7.8.4",
        "@babel/core": "7.9.0",
        "@babel/plugin-proposal-class-properties": "7.8.3",
        "@babel/plugin-proposal-object-rest-spread": "7.9.5",
        "@babel/preset-env": "7.9.5",
        "@babel/preset-flow": "7.9.0",
        "@babel/register": "7.9.0",
        "babel-core": "7.0.0-bridge.0",
        "babel-eslint": "10.1.0",
        "babel-jest": "25.4.0",
        "babel-loader": "8.1.0",
        "babel-plugin-module-resolver": "4.0.0",
        "child_process": "1.0.2",
        "circular-dependency-plugin": "5.2.0",
        "eslint": "6.8.0",
        "eslint-config-prettier": "6.10.1",
        "eslint-import-resolver-babel-module": "5.1.2",
        "eslint-loader": "4.0.0",
        "eslint-plugin-babel": "5.3.0",
        "eslint-plugin-flowtype": "4.7.0",
        "eslint-plugin-import": "2.20.2",
        "eslint-plugin-jest": "23.8.2",
        "eslint-plugin-prettier": "3.1.3",
        "flow-bin": "0.123.0",
        "jest": "25.4.0",
        "jest-junit-reporter": "1.1.0",
        "jsdoc": "3.6.4",
        "prettier": "2.0.4",
        "source-map-loader": "0.2.4",
        "uglifyjs-webpack-plugin": "2.2.0",
        "webpack": "4.43.0",
        "webpack-cli": "3.3.11",
        "webpack-node-externals": "1.7.2"
    },
    "license": "UNLICENSED",
    "jest": {
        "testResultsProcessor": "./node_modules/jest-junit-reporter",
        "moduleDirectories": [
            "node_modules",
            "./src/js"
        ],
        "collectCoverage": true,
        "coverageReporters": [
            "text",
            "html"
        ],
        "collectCoverageFrom": [
            "src/js/**/*.js",
            "!**/node_modules/**"
        ],
        "testPathIgnorePatterns": [
            "testoutput",
            ".generated"
        ]
    },
    "scripts": {
        "docs": "jsdoc -c jsdoc.json && npx opn-cli ./docs/index.html",
        "gen": "sgen -g sgen -g log -g bst -d design.json",
        "build": "NODE_ENV=\"debug\" webpack --display-modules --display-reasons",
        "lint": "eslint src",
        "test": "jest tests",
        "test:watch": "jest --watch tests",
        "test:ci": "jest --ci tests",
        "clean": "rimraf ./docs ./dist ./coverage test-report.xml"
    },
    "dependencies": {
        "@kurlytail/gen-bst": "1.3.131",
        "@kurlytail/gen-lib": "1.3.92",
        "@kurlytail/gen-log": "1.3.149",
        "@kurlytail/gen-sgen": "1.3.105",
        "@kurlytail/gen-pom": "1.3.3",
        "yaml": "1.9.2"
    }
}
